          sum(CASE
                  WHEN (DATEACCT >= $P{datefrom}
                        AND F.FACTACCTTYPE NOT IN('O', 'R', 'C'))
                       OR (DATEACCT = $P{datefrom}
                           AND F.FACTACCTTYPE = NULL) THEN F.AMTACCTDR
                  ELSE 0
              END) AS AMTACCTDR,
          sum(CASE
                  WHEN (DATEACCT >= $P{datefrom}
                        AND F.FACTACCTTYPE NOT IN('O', 'R', 'C'))
                       OR (DATEACCT = $P{datefrom}
                           AND F.FACTACCTTYPE = NULL) THEN F.AMTACCTCR
                  ELSE 0
              END) AS AMTACCTCR,
          F.ACCOUNT_ID AS ACCOUNT_ID,
          to_char('') AS groupbyid
   FROM SBPRF_FACT_ACCT_AGGD F
   WHERE F.AD_ORG_ID IN(select ad_org_id from ad_org)
   --  AND F.AD_CLIENT_ID IN(select ad_client_id from ad_client)
     AND F.AD_ORG_ID IN (select ad_org_id from ad_org)
     AND DATEACCT <= $P{dateto}
    -- AND 1=1
