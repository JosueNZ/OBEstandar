AND f.ad_org_id = CASE WHEN  $P{ad_org_id}  = '0' then f.ad_org_id ELSE  $P{ad_org_id}  END
GROUP BY 1,ACCOUNT_ID,EV.NAME, EV.VALUE, EV.ELEMENTLEVEL,EV.accounttype, f.ad_org_id) A
GROUP BY ad_org_id,ID, NAME, ACCOUNT_ID, ELEMENTLEVEL,accounttype
) MO
FULL OUTER JOIN
(
--SALDO INICIAL A LA FECHA DESDE


SELECT a.ad_org_id,B.VALUE, ACCOUNT_ID, COALESCE(to_number(SUM(AMTACCTDR)),to_number(0)) AS AMTACCTDR, COALESCE(to_number(SUM(AMTACCTCR)),to_number(0)) AS AMTACCTCR,
     COALESCE(SUM(initialamt), 0) AS SALDO_INICIAL
FROM (
   (

    SELECT CASE WHEN $P{ad_org_id} = '0' then f.ad_org_id ELSE $P{ad_org_id} END ad_org_id,sum(CASE
                  WHEN (DATEACCT < $P{datefrom}
                        OR (DATEACCT = $P{datefrom}
                            AND F.FACTACCTTYPE = 'O')) THEN F.AMTACCTDR - F.AMTACCTCR
                  ELSE 0
