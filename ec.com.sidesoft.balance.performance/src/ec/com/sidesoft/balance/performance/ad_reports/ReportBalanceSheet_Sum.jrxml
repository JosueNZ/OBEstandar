     AND F.ISACTIVE='Y'
   GROUP BY F.ACCOUNT_ID, F.AD_ORG_ID
)) A
LEFT JOIN C_ELEMENTVALUE B ON A.ACCOUNT_ID = B.C_ELEMENTVALUE_ID
GROUP BY B.VALUE, ACCOUNT_ID, a.ad_org_id


