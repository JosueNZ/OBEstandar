<!--632EF3673E79433887F13CA48D6F18A9-->  <AD_VAL_RULE_ID><![CDATA[632EF3673E79433887F13CA48D6F18A9]]></AD_VAL_RULE_ID>
<!--632EF3673E79433887F13CA48D6F18A9-->  <AD_CLIENT_ID><![CDATA[0]]></AD_CLIENT_ID>
<!--632EF3673E79433887F13CA48D6F18A9-->  <AD_ORG_ID><![CDATA[0]]></AD_ORG_ID>
<!--632EF3673E79433887F13CA48D6F18A9-->  <ISACTIVE><![CDATA[Y]]></ISACTIVE>
<!--632EF3673E79433887F13CA48D6F18A9-->  <NAME><![CDATA[Invoices from Partner]]></NAME>
<!--632EF3673E79433887F13CA48D6F18A9-->  <DESCRIPTION><![CDATA[Invoices from Partner]]></DESCRIPTION>
<!--632EF3673E79433887F13CA48D6F18A9-->  <TYPE><![CDATA[S]]></TYPE>
<!--632EF3673E79433887F13CA48D6F18A9-->  <CODE><![CDATA[C_Invoice.C_BPartner_ID=@C_BPartner_ID@
and C_Invoice.ad_org_id=@ad_org_id@
and
C_Invoice.Posted='Y'
and C_Invoice.IssoTrx=(case when  @Withholdingtype@='WS' then 'Y' when @Withholdingtype@='WP' then 'N' end) 
AND
(
	(COALESCE((SELECT sum(psd.amount)
	FROM fin_payment_scheduledetail psd
	LEFT JOIN fin_payment_schedule ps ON psd.fin_payment_schedule_invoice = ps.fin_payment_schedule_id
	WHERE ps.c_invoice_id = c_invoice.c_invoice_id AND psd.isinvoicepaid='N' AND psd.fin_payment_detail_id IS NULL ),0)> 0 and @paidinvoice@ = 'N')
OR
	(COALESCE((SELECT sum(psd.amount)
	FROM fin_payment_scheduledetail psd
	LEFT JOIN fin_payment_schedule ps ON psd.fin_payment_schedule_invoice = ps.fin_payment_schedule_id
	WHERE ps.c_invoice_id = c_invoice.c_invoice_id AND psd.isinvoicepaid='N' AND psd.fin_payment_detail_id IS NULL ),0)<= 0 and (@paidinvoice@ = 'Y' or @paidinvoice@ is null))
  
)
   
 and coalesce((select c_invoicetax.c_invoice_id from c_invoicetax 
	join c_tax on c_tax.c_tax_id = c_invoicetax.c_tax_id and c_tax.iswithholdingtax = 'Y' 
	where c_invoicetax.c_invoice_id = c_invoice.c_invoice_id 
 group by  c_invoicetax.c_invoice_id 
	),'ND')= 'ND'  
AND C_Invoice.C_invoice_ID not in (select ssws_withholdingsale.c_invoice_id from ssws_withholdingsale)]]></CODE>
<!--632EF3673E79433887F13CA48D6F18A9-->  <AD_MODULE_ID><![CDATA[436706DE965C4C0C8501EBC805C956DF]]></AD_MODULE_ID>
<!--632EF3673E79433887F13CA48D6F18A9--></AD_VAL_RULE>

