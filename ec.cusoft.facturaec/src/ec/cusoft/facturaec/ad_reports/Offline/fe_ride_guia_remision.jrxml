	</parameter>
	<queryString>
		<![CDATA[SELECT  g.social_name o_socialname, g.name o_name, 
COALESCE(i.address1,'')||'-'||COALESCE(i.address2,'')||'-'||COALESCE(i.postal,'')||'-'||COALESCE(i.city,'')||'-'||COALESCE(rr.name,'')||'-'||COALESCE(cu.name,'') o_address, 
COALESCE(e.address1,'')||'-'||COALESCE(e.address2,'')||'-'||COALESCE(e.postal,'')||'-'||COALESCE(e.city,'')||'-'||COALESCE(r.name,'')||'-'||COALESCE(co.name,'') t_establishmentaddress,
h.duns as o_specialtaxpayer, CASE WHEN k.requiredaccounting='Y' THEN 'SI' ELSE 'NO' END a_obligado_contabilidad, h.taxid o_ruc,
a.documentno t_codedocument, CASE WHEN f.environment = '2' THEN 'PRODUCCIÓN' ELSE 'PRUEBAS' END a_ambiente,
to_char(a.movementdate,'ddmmyyyy')||l.em_eei_edoc_type||h.taxid||f.environment||replace(a.documentno,'-','')
||'12345678'||'1' t_keyaccess, n.taxid t_clientid2, n.name t_clientname, m.description t_plaque,
(SELECT 
COALESCE(cl.address1,'')||'-'||COALESCE(cl.address2,'')||'-'||COALESCE(cl.postal,'')||'-'||COALESCE(cl.city,'')||'-'||COALESCE(rg.name,'')||'-'||COALESCE(ct.name,'')
FROM m_inoutline ml
LEFT JOIN m_locator mo ON ml.m_locator_id = mo.m_locator_id
LEFT JOIN m_warehouse mw ON mw.m_warehouse_id = mo.m_warehouse_id
LEFT JOIN c_location cl ON cl.c_location_id = mw.c_location_id
LEFT JOIN c_region rg ON cl.c_region_id = rg.c_region_id
LEFT JOIN c_country ct ON cl.c_country_id = ct.c_country_id 
WHERE ml.m_inout_id = a.m_inout_id
ORDER BY line LIMIT 1 ) t_splitaddress,
a.movementdate t_fecha_inicio, a.em_smvai_enddateoftransport t_fecha_fin,
(SELECT iv.documentno
FROM c_invoiceline il
INNER JOIN c_invoice iv ON il.c_invoice_id = iv.c_invoice_id
WHERE il.m_inoutline_id = b.m_inoutline_id
ORDER BY line LIMIT 1) c_numbersupportdoc ,
(SELECT iv.dateinvoiced
FROM c_invoiceline il
INNER JOIN c_invoice iv ON il.c_invoice_id = iv.c_invoice_id
WHERE il.m_inoutline_id = b.m_inoutline_id
ORDER BY line LIMIT 1) c_fecha_documento_referenciado,
(SELECT iv.em_eei_codigo
FROM c_invoiceline il
INNER JOIN c_invoice iv ON il.c_invoice_id = iv.c_invoice_id
WHERE il.m_inoutline_id = b.m_inoutline_id
ORDER BY line LIMIT 1) c_numauthsupportdoc,
a.description c_reasontransfer, 
COALESCE(bc.address1,'')||'-'||COALESCE(bc.address2,'')||'-'||COALESCE(bc.postal,'')||'-'||COALESCE(bc.city,'')||'-'||COALESCE(br.name,'')||'-'||COALESCE(bu.name,'') c_address, 
bp.taxid c_recipientidentification, bp.name c_socialname,
a.em_smvai_customcode c_uniquedoccustoms, a.em_smvai_route c_route,
(SELECT mw.em_eei_identifier
FROM m_inoutline ml
LEFT JOIN m_locator mo ON ml.m_locator_id = mo.m_locator_id
LEFT JOIN m_warehouse mw ON mw.m_warehouse_id = mo.m_warehouse_id
WHERE ml.m_inout_id = a.m_inout_id
ORDER BY line LIMIT 1 ) c_estabdestinationcode,
b.movementqty d_invoicedquantity,mp.name d_productdescription, mp.value d_principalcode, mp.value d_auxiliarycode,
COALESCE(bc.address1,'')||'-'||COALESCE(bc.address2,'')||'-'||COALESCE(bc.postal,'')||'-'||COALESCE(bc.city,'')||'-'||COALESCE(br.name,'')||'-'||COALESCE(bu.name,'') t_client_address, 
bpl.phone t_client_telephone, coalesce(bp.em_eei_email,f.default_email) t_client_mail,
coalesce(a.description,'') t_additionalinfovalue1, ''lenguaje,g.ad_org_id,
'FACTURA' c_document
FROM m_inout a
LEFT JOIN m_inoutline b ON a.m_inout_id = b.m_inout_id
LEFT JOIN m_product mp ON b.m_product_id = mp.m_product_id
LEFT JOIN ad_org c ON a.ad_org_id = c.ad_org_id
LEFT JOIN ad_orginfo d ON c.ad_org_id = d.ad_org_id
LEFT JOIN c_location e ON d.c_location_id = e.c_location_id
LEFT JOIN c_region r ON e.c_region_id = r.c_region_id
LEFT JOIN c_country co ON co.c_country_id = e.c_country_id
LEFT JOIN eei_param_facturae f ON f.isactive='Y'
LEFT JOIN c_bpartner bp ON bp.c_bpartner_id = a.c_bpartner_id
LEFT JOIN c_bpartner_location bpl ON bpl.c_bpartner_location_id = a.c_bpartner_location_id
LEFT JOIN c_location bc ON bc.c_location_id = bpl.c_location_id
LEFT JOIN c_region br ON br.c_region_id = bc.c_region_id
LEFT JOIN c_country bu ON bu.c_country_id = bc.c_country_id
--ORG CONTABLE
LEFT JOIN ad_org g ON g.ad_orgtype_id='1'
LEFT JOIN ad_orginfo h ON h.ad_org_id = g.ad_org_id
LEFT JOIN c_location i ON h.c_location_id = i.c_location_id
LEFT JOIN c_region rr ON i.c_region_id = rr.c_region_id
LEFT JOIN c_country cu ON cu.c_country_id = i.c_country_id
LEFT JOIN c_bpartner j ON h.c_bpartner_id = j.c_bpartner_id
LEFT JOIN sswh_taxpayer k ON j.em_sswh_taxpayer_id = k.sswh_taxpayer_id

LEFT JOIN c_doctype l ON a.c_doctype_id = l.c_doctype_id
LEFT JOIN m_shipper m ON a.em_slci_shipper_id = m.m_shipper_id
LEFT JOIN c_bpartner n ON m.c_bpartner_id = n.c_bpartner_id

WHERE a.issotrx='Y' AND a.m_inout_id=$P{DOCUMENT_ID}]]>
	</queryString>
