	<queryString>
		<![CDATA[SELECT
	orden,
	doc documentno,
	factura reference,
	cco pay_documentno,
	tp document_type,
	f_aplicada ref_invoice,
	referencia referenceno,
	emision::DATE dateinvoiced,
	vence::DATE expecteddate,
	concepto description,
	amtacctdr,
	amtacctcr,
	CASE
		WHEN issotrx = 'Y' THEN amtacctdr - amtacctcr
		ELSE amtacctcr - amtacctdr
	END saldo
FROM (
	SELECT DISTINCT
		ci.issotrx,
		ci.c_bpartner_id,
		fa.dateacct,
		fa.account_id,
		CASE
			WHEN cir.c_invoice_id IS NOT NULL THEN 2
			WHEN ir.c_invoice_reverse_id IS NULL THEN 1
			ELSE 2
		END orden,
		COALESCE(r4.documentno, r3.documentno, cir.documentno, ci.documentno) doc,
		ci.poreference factura,
		ci.em_spdn_documentno cco,
		CASE
			WHEN ci.issotrx = 'Y' THEN CASE
				WHEN cir.c_invoice_id IS NOT NULL THEN 'NC'
				WHEN ir.c_invoice_reverse_id IS NULL THEN 'FA'
				WHEN r4.c_invoice_id IS NOT NULL THEN 'ANC'
				ELSE 'AF' END
			ELSE CASE
				WHEN cd.em_sswh_doctype = 'CN' THEN 'NC'
				ELSE COALESCE(cd.em_sswh_doctype,'FA') END
		END tp,
		CASE WHEN cir.c_invoice_id IS NOT NULL THEN ci.documentno ELSE NULL END f_aplicada,
		'' referencia,
		ci.dateinvoiced emision,
		fpi.expecteddate vence,
		ci.description concepto,
		fa.amtacctdr,
		fa.amtacctcr
	FROM c_invoice ci
		JOIN c_doctype cd ON cd.c_doctype_id = ci.c_doctype_id
		JOIN fact_acct fa ON fa.ad_table_id = '318'
			AND fa.record_id = ci.c_invoice_id
		JOIN (
			SELECT c_invoice_id, MAX(expecteddate) expecteddate
			FROM fin_payment_schedule
			WHERE c_invoice_id IS NOT NULL
			GROUP BY c_invoice_id
		) fpi ON fpi.c_invoice_id = ci.c_invoice_id
		LEFT JOIN c_invoice cir ON cir.c_invoice_id = ci.em_scnr_invoice_id
		LEFT JOIN c_invoice_reverse ir ON ir.c_invoice_id = ci.c_invoice_id
		LEFT JOIN c_invoice r3 ON r3.c_invoice_id = ir.reversed_c_invoice_id
		LEFT JOIN c_doctype dt3 ON dt3.c_doctype_id = r3.c_doctype_id
		LEFT JOIN c_invoice r4 ON r4.c_invoice_id = r3.em_scnr_invoice_id
	WHERE ci.docstatus != 'DR'
		AND $P!{accounts_query_inv}
		AND ci.issotrx = $P!{issotrx_str}
		AND ci.c_bpartner_id = $P!{bpartner_id_query}
		AND fa.dateacct >= TO_DATE($P!{date_fromstr})
		AND fa.dateacct <= TO_DATE($P!{date_tostr})

	UNION

	SELECT ci3.issotrx,
		ci3.c_bpartner_id,
		fa3.dateacct,
		fa3.account_id,
		3 orden,
		ci3.documentno doc,
		ci3.poreference factura,
		wh.documentno cco,
		'RTV' tp,
		'' f_aplicada,
		'' referencia,
		wh.withholdingdate emision,
		fpi1.expecteddate vence,
		wh.description concepto,
		SUM(fa3.amtacctdr) amtacctdr,
		SUM(fa3.amtacctcr) amtacctcr
	FROM ssws_withholdingsale wh
		JOIN fact_acct fa3 ON fa3.ad_table_id = '211492B753264EAEBE328BA4FED1F066'
			AND fa3.record_id = wh.ssws_withholdingsale_id
		JOIN c_invoice ci3 ON ci3.c_invoice_id = wh.c_invoice_id
		JOIN (
			SELECT c_invoice_id, MAX(expecteddate) expecteddate
			FROM fin_payment_schedule
			WHERE c_invoice_id IS NOT NULL
			GROUP BY c_invoice_id
		) fpi1 ON fpi1.c_invoice_id = ci3.c_invoice_id
	WHERE ci3.docstatus != 'DR'
		AND $P!{accounts_query_rtv}
		AND ci3.issotrx = $P!{issotrx_str}
		AND ci3.c_bpartner_id = $P!{bpartner_id_query}
		AND fa3.dateacct::DATE >= TO_DATE($P!{date_fromstr})
		AND fa3.dateacct::DATE <= TO_DATE($P!{date_tostr})
	GROUP BY ci3.issotrx, ci3.c_bpartner_id, fa3.dateacct, fa3.account_id, doc,
		factura, cco, emision, vence, concepto

	UNION

	SELECT fp.isreceipt issotrx,
		fp.c_bpartner_id,
		fa2.dateacct,
		fa2.account_id,
		4 orden,
		ci2.documentno doc,
		ci2.poreference factura,
		fp.documentno cco,
		'PA' tp,
		'' f_aplicada,
		fp.referenceno referencia,
		fp.paymentdate emision,
		fps2.expecteddate vence,
		COALESCE(fp.description,'-') || ' / ' || COALESCE(fa.name,'-') || ' / ' || COALESCE(bt.name,'-') || ' / ' || COALESCE(fp.em_ecscap_no_check,'-') concepto,
		SUM(fa2.amtacctdr) amtacctdr,
		SUM(fa2.amtacctcr) amtacctcr
	FROM fin_payment fp
		JOIN fin_payment_detail fpd ON fpd.fin_payment_id = fp.fin_payment_id
		JOIN fact_acct fa2 ON fa2.ad_table_id = 'D1A97202E832470285C9B1EB026D54E2'
			AND fa2.record_id = fp.fin_payment_id
			AND fa2.line_id = fpd.fin_payment_detail_id
		JOIN fin_financial_account fa ON fa.fin_financial_account_id = fp.fin_financial_account_id
		LEFT JOIN ssfi_banktransfer bt ON bt.ssfi_banktransfer_id = fp.EM_Ecscap_Banktransfer_ID
		JOIN fin_payment_scheduledetail fps ON fps.fin_payment_detail_id = fpd.fin_payment_detail_id
			AND fps.em_ssws_withholdingsale_id IS NULL
		JOIN fin_payment_schedule fps2 ON fps2.fin_payment_schedule_id = fps.fin_payment_schedule_invoice
		JOIN c_invoice ci2 ON ci2.c_invoice_id = fps2.c_invoice_id
		LEFT JOIN c_invoice r2 ON r2.c_invoice_id = ci2.em_scnr_invoice_id
		LEFT JOIN c_invoice_reverse ir ON ir.c_invoice_id = ci2.c_invoice_id
		LEFT JOIN c_invoice r3 ON r3.c_invoice_id = ir.reversed_c_invoice_id
		LEFT JOIN c_invoice r4 ON r4.c_invoice_id = r3.em_scnr_invoice_id
	WHERE ci2.docstatus != 'DR' AND fp.processed = 'Y'
		AND r2.c_invoice_id IS NULL AND ir.c_invoice_id IS NULL
		AND $P!{accounts_query_pymt}
		AND fp.isReceipt = $P!{issotrx_str}
		AND fp.c_bpartner_id = $P!{bpartner_id_query}
		AND fa2.dateacct::DATE >= TO_DATE($P!{date_fromstr})
		AND fa2.dateacct::DATE <= TO_DATE($P!{date_tostr})
	GROUP BY fp.isreceipt, fp.c_bpartner_id, fa2.dateacct, fa2.account_id, doc,
		factura, cco, referencia, emision, vence, concepto

) ta
/*
WHERE c_bpartner_id = '852A98D9E7264DBD9D99E8CD1AAF78CE'
	AND dateacct::DATE >= '2023-01-01'::DATE AND dateacct::DATE <= NOW()::DATE
	AND account_id = 'E7AE328FE42F438E8F29F290901C7C49'
*/
ORDER BY issotrx, doc, orden, emision]]>
	</queryString>
