	<queryString>
		<![CDATA[SELECT
    t.ad_org_id AS organizationid
    , t.organization
    , t.f_perioname
    , t.fecha_impresion
    , t.f_value
    , (SELECT a.name FROM c_elementvalue a JOIN ad_org b ON b.ad_org_id=t.ad_org_id WHERE a.value=t.f_value) AS f_name
    , t.accounttype
    , SUM(t.f_deb) AS f_deb
    , SUM(t.f_acred) AS f_acred
    , SUM(t.f_debinicial) AS f_debinicial
    , SUM(t.f_creinicial) AS f_creinicial
    , SUM(t.f_debfinal) AS f_debfinal
    , SUM(t.f_crefinal) AS f_crefinal
    , SUM(t.f_dedacc) AS f_dedacc
    , SUM(t.f_creacc) AS f_creacc

, t.label_balance_comprobacion
, t.label_periodo
, t.label_codigo
, t.label_nombre
, t.label_saldo_inicial
, t.label_deudor
, t.label_acreedor
, t.label_flujos
, t.label_debe
, t.label_haber
, t.label_sumas
, t.label_saldos_finales
, t.label_totales
, t.nombre_firma_1
, t.nombre_firma_2
, t.nombre_firma_3
, t.nombre_firma_4
, t.nombre_cargos_1
, t.nombre_cargos_2
, t.nombre_cargos_3
, t.nombre_cargos_4
,CASE
    WHEN $P{Periodfrom} IS NULL OR $P{Periodfrom} = '' THEN
        'Al '||TO_CHAR(last_day(NOW()::DATE), 'DD')||' de '||sescr_monthtoletters(to_number(TO_CHAR(NOW(), 'MM')))||' de '||to_char(NOW(),'YYYY')
    ELSE
        'Al '||TO_CHAR(last_day(
        ((SELECT year FROM c_year WHERE c_year_id = $P{C_Year_ID})||'-'||
        (SELECT periodno FROM c_period WHERE c_period_id=$P{Periodfrom})||'-01')
        ::DATE), 'DD')||' de '||sescr_monthtoletters(to_number(
        (SELECT periodno FROM c_period WHERE c_period_id=$P{Periodfrom})
         ))||' de '||(SELECT year FROM c_year WHERE c_year_id = $P{C_Year_ID})
END AS periodo
FROM (
select
	myacc.ad_org_id
	,myacc.organization
	,coalesce(aa.f_deb,0) as f_deb-- Flujo Deudor
	, coalesce(aa.f_acred,0) as f_acred-- Flujo Acreedor
	, aa.f_accountid
	--, aa.f_year
	,('1 DE ENERO '||audit.f_year||' AL '||to_char(audit.fecha_reporte,'DD')||' DE '||UPPER(sescr_monthtoletters(to_number(to_char(audit.fecha_reporte,'MM'))))||' '||audit.f_year) as f_perioname
	,audit.fecha_impresion
	, CASE
		WHEN myacc.account::NUMERIC < 0 THEN myacc.value
		ELSE myacc.account
    	END AS f_value
	--,myacc.value as f_value
	--,myacc.f_name as f_name
	,coalesce(sum(bb.debim),0) as f_debInicial -- Saldo Inicial Deudor
	,coalesce(sum(abim),0) as f_creInicial -- Saldo Inicial Acreedor
	,coalesce((case when ((coalesce (sum(coalesce(bb.debim,0)),0) +  coalesce(aa.f_deb,0) ) - (coalesce (sum(coalesce(abim,0)), 0) +  coalesce(aa.f_acred,0))) > 0
		then ((coalesce (sum(coalesce(bb.debim,0)), 0) +  coalesce(aa.f_deb, 0) ) - (coalesce (sum(coalesce(abim,0)), 0) +  coalesce(aa.f_acred,0))) end ),0) as f_debfinal --saldo Final
	,coalesce((case when ((coalesce (sum(coalesce(bb.debim,0)),0) +  coalesce(aa.f_deb,0) ) - (coalesce (sum(coalesce(abim,0)), 0) +  coalesce(aa.f_acred,0))) < 0
		then  abs((coalesce (sum(coalesce(bb.debim,0)),0) +  coalesce(aa.f_deb,0) ) - (coalesce (sum(coalesce(abim,0)), 0) +  coalesce(aa.f_acred,0))) end), 0) as f_crefinal -- Saldo Final Acreedor
	,coalesce (coalesce (sum(coalesce(bb.debim,0)),0) + coalesce(aa.f_deb,0) , 0) as f_dedAcc-- Acumuldao Deudor
	,coalesce (coalesce (sum(coalesce(bb.abim,0)), 0) +  coalesce(aa.f_acred,0), 0) as f_creAcc --Acumulado Acreedor
	,myacc.accounttype
,labels.*
,signing.*
from (select a.c_elementvalue_id
	,sscmu_accounttreelevel($P{LEVEL}, a.c_elementvalue_id) as account
	,a.value
	,a.name as f_name
	,a.accounttype
	,b.name as "organization"
	,b.ad_org_id
from c_elementvalue a
join ad_org b on b.ad_org_id=a.ad_org_id
) myacc
left join (select
		coalesce(sum(fa.amtacctdr), 0) as f_deb --Flijo deudor
		,coalesce(sum(fa.amtacctcr), 0) as f_acred --Flujo Acreedor
		,fa.account_id as f_accountid
		,ye.year as f_year--,  pe.name as f_perioname
		,el.value || '/' || el.name as f_value
		,coalesce (zz.debi, 0) as  f_debInicial
		,coalesce(zz.abi, 0)  f_creInicial
		--,pe.periodno
		,el.accounttype
	from fact_acct fa
		inner join c_period pe on pe.c_period_id = fa.c_period_id
		inner join c_year ye on ye.c_year_id = pe.c_year_id
		inner join C_ElementValue el on el.C_ElementValue_id = fa.account_id
		left join (select sum(fac1.amtacctdr) debi
					,  sum(fac1.amtacctcr) as abi
					,account_id
				from  fact_acct fac1
					inner join c_period pe1 on pe1.c_period_id = fac1.c_period_id
				where  pe1.c_year_id in (select c_year_id from c_year where c_year.created < (select created from c_year where c_year_id = $P{C_Year_ID}))
				group by  account_id
			) as zz on  zz.account_id =  fa.account_id
	where ye.c_year_id = $P{C_Year_ID} and  (( pe.periodno <= (select pp.periodno from c_period pp where pp.c_period_id= $P{Periodfrom}) OR $P{Periodfrom} is null))
        group by  fa.account_id ,ye.year, el.value, el.name, zz.debi, zz.abi,el.accounttype
       ) aa on myacc.C_elementvalue_id = aa.f_accountid
	left join (select sum(fac01.amtacctdr) debim
			, sum(fac01.amtacctcr) as abim
			, fac01.account_id
		from fact_acct fac01
			inner join c_period pe01 on pe01.c_period_id = fac01.c_period_id
			join c_year y on y.c_year_id=pe01.c_year_id
		where y.year <  (select year from c_year where c_year_id= $P{C_Year_ID})
		group by fac01.account_id
	) as bb on (bb.account_id = myacc.C_elementvalue_id)
,(Select sescr_get_label_trl('Label_FactAccout_TrialBalance','M',$P{LANGUAGE}) as "label_balance_comprobacion"
	,sescr_get_label_trl('Label_FactAccout_Period','M',$P{LANGUAGE}) as "label_periodo"
	,sescr_get_label_trl('Label_FactAccout_Code','M',$P{LANGUAGE}) as "label_codigo"
	,sescr_get_label_trl('Label_FactAccout_Name','M',$P{LANGUAGE}) as "label_nombre"
	,sescr_get_label_trl('Label_FactAccout_InitialBalance','M',$P{LANGUAGE}) as "label_saldo_inicial"
	,sescr_get_label_trl('Label_FactAccout_Debtor','M',$P{LANGUAGE}) as "label_deudor"
	,sescr_get_label_trl('Label_FactAccout_Creditor','M',$P{LANGUAGE}) as "label_acreedor"
	,sescr_get_label_trl('Label_FactAccout_Flows','M',$P{LANGUAGE}) as "label_flujos"
	,sescr_get_label_trl('Label_FactAccout_Debit','M',$P{LANGUAGE}) as "label_debe"
	,sescr_get_label_trl('Label_FactAccout_Credit','M',$P{LANGUAGE}) as "label_haber"
	,sescr_get_label_trl('Label_FactAccout_Sums','M',$P{LANGUAGE}) as "label_sumas"
	,sescr_get_label_trl('Label_FactAccout_FinalBalances','M',$P{LANGUAGE}) as "label_saldos_finales"
	,sescr_get_label_trl('Label_FactAccout_Totals','M',$P{LANGUAGE}) as "label_totales"
From Dual) labels
,(Select
	sy.year as "f_year"
	,(select to_char(now(),'dd/MM/yyyy HH24:MI:SS')) as "fecha_impresion"
	,(CASE WHEN coalesce(sp.periodno,12)<12Then
		to_date('01-'||(CASE WHEN sp.periodno+1<10 Then '0' else ''End) ||sp.periodno+1||'-'||sy.year) - Interval '1 day'
	ELSE
		to_date('01-01-'||to_number(sy.year)+1) - Interval '1 day'
	END) as "fecha_reporte"
From c_year sy
	Left Join c_period sp on sp.c_year_id = sy.c_year_id
		And sp.c_period_id =$P{Periodfrom}
Where sy.c_year_id=$P{C_Year_ID}
) audit
,(select split_part( bb.nombres, ',-,', 1) as "nombre_firma_1"
	,split_part( bb.nombres, ',-,', 2) as "nombre_firma_2"
	,split_part( bb.nombres, ',-,', 3) as "nombre_firma_3"
	,split_part( bb.nombres, ',-,', 4) as "nombre_firma_4"
	,split_part( bb.cargos, ',-,', 1) as "nombre_cargos_1"
	,split_part( bb.cargos, ',-,', 2) as "nombre_cargos_2"
	,split_part( bb.cargos, ',-,', 3) as "nombre_cargos_3"
	,split_part( bb.cargos, ',-,', 4) as "nombre_cargos_4"
FROM (Select STRING_AGG(coalesce(y.name,' '),to_char(',-,') ORDER BY y.line ASC) as "nombres"
	,STRING_AGG(coalesce(y.position,' '),to_char(',-,') ORDER BY y.line ASC) as "cargos"
From c_doctype z left join scsdc_signaturesperdoc y on y.c_doctype_id = z.c_doctype_id
Where UPPER(z.name) =UPPER('ESTADOS FINANCIEROS') -- TIPO DE DOCUMENTO QUEMADO
) bb) signing
WHERE (myacc.C_elementvalue_id  = bb.account_id  or myacc.C_elementvalue_id = aa.f_accountid)
group by aa.f_accountid ,audit.f_year,myacc.value,myacc.f_name, aa.f_debInicial, aa.f_creInicial,aa.f_deb, aa.f_acred, bb.account_id,myacc.accounttype,myacc.ad_org_id,myacc.organization
,labels.label_balance_comprobacion,labels.label_periodo,labels.label_codigo,labels.label_nombre,labels.label_saldo_inicial,labels.label_deudor
,labels.label_acreedor,labels.label_flujos,labels.label_debe,labels.label_haber,labels.label_sumas,labels.label_saldos_finales,labels.label_totales,audit.fecha_impresion,signing.nombre_firma_1,signing.nombre_firma_2,signing.nombre_firma_3,signing.nombre_firma_4,signing.nombre_cargos_1,signing.nombre_cargos_2,signing.nombre_cargos_3,signing.nombre_cargos_4,audit.fecha_reporte,myacc.c_elementvalue_id,myacc.account
) AS t
GROUP BY
    ad_org_id
    , organization
    , f_perioname
    , fecha_impresion
    , f_value
    , f_name
    , accounttype

, label_balance_comprobacion
, label_periodo
, label_codigo
, label_nombre
, label_saldo_inicial
, label_deudor
, label_acreedor
, label_flujos
, label_debe
, label_haber
, label_sumas
, label_saldos_finales
, label_totales
, nombre_firma_1
, nombre_firma_2
, nombre_firma_3
, nombre_firma_4
, nombre_cargos_1
, nombre_cargos_2
, nombre_cargos_3
, nombre_cargos_4
Order By f_value, f_name]]>
	</queryString>
